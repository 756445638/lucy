		ok = false;
		return ;
	}
	if false != a <= b  {
		ok = false;
		return ;
	}
	if false != (a == b)  {
		ok = false;
		return ;
	}
	if  true != (a != b)   {
		ok = false;
		return ;
	}
}






fn testFloats(){
	if false == testFloat() {
		panic(new error("test float failed"));
	}
    if false == testDouble()  {
		panic(new error("test double failed"));
	}
}


