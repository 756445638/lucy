




fn testFloat()->(ok bool = false){
	var a ,b float
	a = 2.0
	b = 1.0
	if true != a > b {
		return 
	}
	if true != a >= b {
		return 
	}
	if false !=  a < b {
		return 
	}
	if false != a <= b  {
		return 
	}
	if false != (a == b)  {
		return 
	}
	if  true != (a != b)   {
		return 
	}
	ok = true 
}



fn testDouble()->(ok bool = false){
	var a ,b double
	a = 2.0
	b = 1.0
	if true != a > b {	
		return 
	}
	if true != a >= b {	
		return 
	}
	if false !=  a < b {
		return 
	}
	if false != a <= b  {
		return 
	}
	if false != (a == b)  {
		return 
	}
	if  true != (a != b)   {
		return 
	}
	ok = true 
}






fn testFloats(){
	if false == testFloat() {
		panic(new error("test float failed"))
	}
    if false == testDouble()  {
		panic(new error("test double failed"))
	}
}


