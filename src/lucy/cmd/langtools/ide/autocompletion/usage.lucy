import "lucy/os"


const usage = `
    show auto completion items.
       -file   file to be located
       -buffer-file the file been editing
       -line   which line (zero based)
       -column which column (zero based)
`

fn printUsageAndExit(code int ){
    print(usage)
    os.exit(code)
}
