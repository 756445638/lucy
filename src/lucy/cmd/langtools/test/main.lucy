import "lucy/cmd/langtools/common"
import "lucy/os"


fn printUsageAndExit(code int){
	print("expect package name")	
	os.exit(code)
}



fn main(args []string){
	if args.size() == 0 {
		printUsageAndExit(1)
	}
	lucyRoot,err := common.getLucyRoot()
	if err != null {
		print(err)
		return 
	}
	lcyPaths ,err := common.getLucyPath()
	if err != null {
		print(err)
		return 
	}
	new Test(lucyRoot , lcyPaths ,args[0]) .test()
}










