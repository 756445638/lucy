import "lucy/cmd/langtools/common";
import "lucy/os";


fn printUsagaAndExit(code int){
	print("expect package name");	
	os.exit(code);
}






fn main(args []string){
	if args.size() == 0 {
		printUsagaAndExit(1);
	}
	lucyroot,err := common.getLucyRoot();
	if err != null {
		print(err);
		return ;
	}
	lcypaths ,err := common.getLucyPath();
	if err != null {
		print(err);
		return ;
	}
	(new Test(lucyroot , lcypaths ,args[0]) ).test();
}










