import "lucy/cmd/langtools/common"
import "lucy/os"


fn printUsageAndExit(code int){
	print("expect package name")	
	os.exit(code)
}


public fn test(args []string) {
	if args.size() == 0 {
		printUsageAndExit(1)
	}
	lucyRoot := common.getLucyRoot()
	lcyPaths  := common.getLucyPath()
	new Test(lucyRoot , lcyPaths ,args[0]) .test()
}


fn main(args []string){
	test(args)
}

