//represent a array or a map build in method 
public class ArrayMapBuildInMethod {
	Type   string // array or map right now
	method string
	pos    Pos    // not defined

	fn ArrayMapBuildInMethod(
		Type string,
		method string) {
		this.super()
		this.Type = Type
		this.method = method
	}

	public fn mkDescription() -> (s string) {
		s = sprintf("build in '%s'" , this.method)
	}
}

