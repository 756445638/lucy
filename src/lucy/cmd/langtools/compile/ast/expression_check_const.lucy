 

class ExpressionCheckConst {
	
	static fn checkConstant(e Expression , block Block) []error {
		errs := []error{}
		cs := e.Data.([]Constant)
		for _, c := range cs {
			err := checkConst(block, c)
			if err != null {
				if err != null {
					errs.append(err)
				}
				continue
			}
			err = block.Insert(c.Name, c.Pos, c)
			if err != null {
				errs.append(err)
			}
		}
		return errs
	}
}

