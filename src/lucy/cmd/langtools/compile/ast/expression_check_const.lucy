class ExpressionCheckConst {
    static fn check(e Expression , block Block) -> (errs []error = []error{}) {
        cs := e.data.([]Constant)
        for _ , c := range cs {
            err := checkConst(block , c , errs)
            if err != null {
                errs.append(err)
                continue
            }
            err = block.insert(c.name , c.pos , c)
            if err != null {
                errs.append(err)
            }
        }
        return errs
    }
}


