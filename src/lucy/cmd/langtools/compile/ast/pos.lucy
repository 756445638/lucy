 

class Pos {
	Filename string
	Line     int
	Column   int
	/*
		offset at bs , for special useã€‚
		for template function only currently
	*/
	Offset int
	fn ErrMsgPrefix() -> (prefix string) {
		return sprintf("%s:%d:%d:", this.Filename, this.Line, this.Column)
	}
}



class NameWithPos {
	Name string
	Pos  Pos
}

fn errMsgPrefix(pos Pos) -> (prefix string) {
	return pos.ErrMsgPrefix()
}
