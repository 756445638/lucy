



enum  resourceKind{
	resourceKindJavaClass = 1,
	resourceKindJavaPackage,
	resourceKindLucyClass,
	resourceKindLucyPackage
}

class Resource {
	kind     resourceKind
	realPath string
	name     string
	fn Resource(kind resourceKind ,realPath string , name     string) {
	    this.super()
	    this.kind = kind
	    this.realPath = realPath
	    this.name = name
	}
}
