


public class AttributeInfo {
	public	NameIndex       char
	attributeLength int
	Info            []byte
}

typealias AttributeGroupedByName =  map {string -> []AttributeInfo }


fn GetAttributeByName(a AttributeGroupedByName , name string) -> (attrs []AttributeInfo) {
	if a == null {
		return null
	}
	return a[name]
}








