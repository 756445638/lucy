import "lucy/binary"
 

class AttributeSourceFile {
	file string
	fn AttributeSourceFile(file string){
        this.super()
        this.file = file
	}
	fn toAttributeInfo(c Class) -> (ret AttributeInfo) {
		ret = new AttributeInfo()
		ret.nameIndex = c.insertUtf8Const(AttributeNameSourceFile)
		ret.attributeLength = 2
		ret.info = new []byte(2)
		binary.BigEndian.putChar(ret.info, c.insertUtf8Const(this.file))
		return ret
	}
}

