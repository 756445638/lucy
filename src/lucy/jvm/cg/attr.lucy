      


public class AttributeInfo {
	public	nameIndex       char
	attributeLength int
	public info            []byte
}

typealias AttributeGroupedByName =  map {string -> []AttributeInfo }

public fn getAttributeByName(a AttributeGroupedByName , name string) -> (attrs []AttributeInfo) {
	if a == null {
		return null
	}
	return a[name]
}








