


import "lucy/os/flag"



fn main(args []string) {
	flag := new flag.FlagParser()
	b := flag.boolOption("-b",false,"bool")
	i := flag.intOption("-i" , 123 ,"")
	
	f := flag.floatOption("-f" , 1f, "")
	no,_ := flag.parse(["-b","-i" ,"-456" , "121" ,"-aaa" , "-f" ,"-93232.21"])

	print(no)
	print(b.getBoolValue())
	print(i.getIntValue())
	print(f.getFloatValue())
	print(flag.helpMsg());
}






















