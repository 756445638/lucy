
enum OptionType {
	OptionTypeBool = 1 ,
	OptionTypeInt ,
	OptionTypeString ,
	OptionTypeFloat 
}


public abstract class Option {
	protected  typ OptionType
	protected usage string 
	
	protected abstract fn formatDefaultValue() -> ( format string  ) 

	protected abstract fn reset() 
		
	protected abstract fn parse(arg string) -> (err error )  
	
}




