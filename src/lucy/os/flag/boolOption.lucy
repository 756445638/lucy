


public final class BoolOption  extends Option{
	private boolDefaultValue bool 
	private boolValue bool 
	fn reset() {
		this.boolValue = this.boolDefaultValue 
	}
	fn parse(s string) -> (err error) {
		this.boolValue = !this.boolDefaultValue
	} 
	public fn getBoolValue() -> (b bool){
		b = this.boolValue
	}
}